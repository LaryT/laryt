<script>
var prefs = new gadgets.Prefs();

var mKey = prefs.getString("m_Key");
if(mKey === ""){
 mKey="0AjrqMWGJ6suYdHpjTFktMVF2bjdsdHkwaE5aQk83d3c";
}
var mDiorganvsh = prefs.getString("m_Diorganvsh");
