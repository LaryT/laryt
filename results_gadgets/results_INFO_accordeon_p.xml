function scroll(id, direction, stepPX)
{
  var scrollTo = $("#fpOtherInner_" + id).scrollTop() + stepPX * direction;
  var scrollHeight = $("#fpOtherInner_" + id)[0].scrollHeight;
  var height = $("#fpOtherInner_" + id).height();
  if (scrollTo < 0) scrollTo = 0;
  if (scrollTo > scrollHeight - height) scrollTo = scrollHeight - height;
  $("#fpOtherInner_" + id).animate({scrollTop: scrollTo}, 300);
}

function scrolltoAgonistikh(id, stepPX)
{
  var scrollTo=stepPX;
  var scrollHeight = $("#fpOtherInner_" + id)[0].scrollHeight;
  var height = $("#fpOtherInner_" + id).height();
  if (scrollTo < 0) scrollTo = 0;
  if (scrollTo > scrollHeight - height) scrollTo = scrollHeight - height;
  $("#fpOtherInner_" + id).animate({scrollTop: scrollTo}, 3000);
}


