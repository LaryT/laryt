  }esle if(data.getValue(row, 6) = 3){
    html.push('<div class="clMetalioXalkino clSpanOnomateponymo clDivLeft boxOut10black">'+data.getFormattedValue(row, 6)+'</div>');


  html.push('<div class="clMetalioXryso clSpanOnomateponymo clDivLeft boxOut10black">'+data.getFormattedValue(row, 6)+'</div>');
  */
  html.push('<div class="clDivLeft"><img class="imgFlagRadius boxOut10black" src="'+data.getFormattedValue(row, 9)+'" /></div>')
  html.push('<div class="clSpanOnomateponymo clDivLeft">'+data.getFormattedValue(row, 1).toUpperCase() + '</div>');
  html.push('<div class="clDivLeft" >('+ data.getFormattedValue(row, 2) +')</div>');
  html.push('</div>');
  

  html.push('</div>');

}
