var mCountry = prefs.getString("m_Country");

var urlMaxiCategories = "https://docs.google.com/spreadsheet/ccc?key="+ mKey +"&headers=1";
var queryMaxiCategories;  
  
function myInitMaxiAge(){

  if( urlMaxiCategories!="" ){
     queryMaxiCategories = new google.visualization.Query(urlMaxiCategories);
     sendQueryMaxiCategories();
  }else{
   alert('No source file for  Categories');
  }
 
}
 
function sendQueryMaxiCategories(){
  if( mCategory!="" ){
   var strQ='select A,B,C,D,E,F,G,H,I,J,K,L where C="'+mCategory +'"  order by H,D,G';
  }else{
   var strQ='select A,B,C,D,E,F,G,H,I,J,K,L order by H,D,G';
   
  }
   queryMaxiCategories.setQuery(strQ);   
   queryMaxiCategories.send(handleResponseQueryMaxiCategories);
}
 
function handleResponseQueryMaxiCategories(response) {

 if (response.isError()) {
     alert('Error in query_members: ' + response.getMessage() + ' ' + response.getDetailedMessage());
     return;
 }
 var data = response.getDataTable();
 var posesEggrafes = data.getNumberOfRows();

alert(data.getFormattedValue(3, 6));

if(posesEggrafes > 0) {
 var html =[]; 
 var strGroup1 = data.getFormattedValue(0, 0).toUpperCase(); 
 var strGroup2 = data.getFormattedValue(0, 3).toUpperCase();
 var myCount=0;
 html.push('<h3 class="myH3title font-effect-emboss"><img class="imgLogoRadius boxOut10" src="'+data.getFormattedValue(0,8)+'" />'+strGroup1+'</h3><div>');
 for (var row = 0; row < posesEggrafes; row++) {
  if( strGroup1 != data.getFormattedValue(row, 0).toUpperCase() ){
   strGroup1 = data.getFormattedValue(row, 0).toUpperCase();
   html.push('</div><h3 class="myH3title font-effect-emboss"><img class="imgLogoRadius boxOut10" src="'+data.getFormattedValue(row, 8)+'" />'+strGroup1+'</h3><div>');
   myCount=0;
  }
  myCount++;
  html.push('<div>');
  

  if( strGroup2 != data.getFormattedValue(row, 3).toUpperCase() ){
   strGroup2 = data.getFormattedValue(row, 3).toUpperCase();
   html.push('<h3 class="myH3title font-effect-emboss">'+strGroup2+'<img class="imgCategoryRadius" src="'+data.getFormattedValue(row, 10)+'" /></h3>');
  }
  
  html.push('<div class="clDivData">');
  

  if(data.getValue(row, 6) < 2 ){
    html.push('<div class="clMetalioXryso clSpanOnomateponymo clDivLeft boxOut10black">'+data.getFormattedValue(row, 6)+'</div>');
  }else{
    html.push('<div class="clSpanOnomateponymo clDivLeft boxOut10black">'+data.getFormattedValue(row, 6)+'</div>');
  }
 
/*

 }else if(data.getValue(row, 6) = 2){
